{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "OlympusAI",
  "colors": {
    "primary": "#0D9373",
    "light": "#07C983",
    "dark": "#0D9373"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "User Guide",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart"
            ]
          },
          {
            "group": "App Modules",
            "pages": [
              "modules/dashboard",
              "modules/brand",
              "modules/offers",
              "modules/integrations"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "App",
          "href": "https://app.etho.net/",
          "icon": "rocket"
        },
        {
          "anchor": "Support",
          "href": "mailto:support@etho.net",
          "icon": "life-ring"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
